{
  "title": "Navigation",
  "rules": [
    {
      "description": "rotate tab next",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": { "mandatory": [ "left_shift", "left_control" ] }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers":  [ "left_control" ]
            }
          ]
        }
      ]
    },
    {
      "description": "rotate tab previous",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": { "mandatory": [ "left_shift", "left_control" ] }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers":  [ "left_control", "left_shift" ]
            }
          ]
        }
      ]
    },
